funct fibonacci(n) {
  var x = 1;
  var x_1 = 1;
  var x_2 = 0;
  
  for(var i = 2; i < n; i = i + 1) {
    var temp_x = x;
    x = x + x_1;
    x_2 = x_1;
    x_1 = temp_x;
  }

  return x;
}

funct fib_recursive(n) {
  if (n <= 1) {
    return 1;
  }
  return fib_recursive(n - 1) + fib_recursive(n - 2);
}

print fibonacci(5);
print fib_recursive(5);
